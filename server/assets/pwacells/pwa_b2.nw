function spawnDrop() {
  this.pl[i] = 5;
  this.px[i] = random(0,64);
  this.py[i] = random(0,64);
  this.pang[i] = random(pi/4,pi/2);
}

function moveDrop() {
  this.px[i] += cos(this.pang[i]);
  this.py[i] += sin(this.pang[i]);
  this.pl[i] -= 0.1;
}

function renderDrop() {
  showimg 300+i,pwa_raindrop.png,this.px[i],this.py[i];
}

function lightning() {
  seteffect 0,0,0,0.25;
  this.rand = int(random(0,100));
  if (this.rand > 98) {
    play thunder.wav;
    seteffect 1,1,1,0.9;
  } else {
    play rain.wav;
  }
}
